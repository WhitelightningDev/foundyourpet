# Frontend environment variables

# Backend base URL (local example)
# REACT_APP_API_BASE_URL=http://localhost:5001

# Web Push (VAPID public key)
REACT_APP_WEB_PUSH_PUBLIC_KEY=

# Optional: Firebase (FCM) for app-style tokens / background notifications in supported browsers
REACT_APP_FIREBASE_API_KEY=
REACT_APP_FIREBASE_AUTH_DOMAIN=
REACT_APP_FIREBASE_PROJECT_ID=
REACT_APP_FIREBASE_STORAGE_BUCKET=
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=
REACT_APP_FIREBASE_APP_ID=
REACT_APP_FIREBASE_VAPID_KEY=

# Optional: override API paths
# REACT_APP_WEB_PUSH_SUBSCRIBE_PATH=/api/notifications/webpush/subscribe
# REACT_APP_NOTIFICATIONS_REGISTER_PATH=/api/notifications/register
# REACT_APP_PUBLIC_PET_REPORT_PATH=/api/reports/public-pet
# REACT_APP_PUBLIC_REPORTS_FEED_PATH=/api/reports/public

